	},
	"item": [
		{
			"name": "Generate Token",
			"request": {
				"method": "POST",
				"header": [],
				"url": {
					"raw": "{{url}}/api/sanctum/token?email=useremail@gmail.com&password=userpass&device_name=postman",
					"host": [
						"{{url}}"
					],
					"path": [
						"api",
						"sanctum",
						"token"
					],
					"query": [
						{
							"key": "email",
							"value": "useremail@gmail.com"
						},
						{
							"key": "password",
							"value": "userpass"
						},
						{
							"key": "device_name",
							"value": "postman"
						}
					]
				},
				"description": "Returns a bearer authentication token given a valid user's email and password."
			},
			"response": []
		}
	]
